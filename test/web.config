       bypassProxyOnLocal="false" hostNameComparisonMode="StrongWildcard"
       keepAliveEnabled="true" maxBufferSize="65536" proxyAuthenticationScheme="Anonymous"
       realm="" transferMode="Buffered" unsafeConnectionNtlmAuthentication="false"
       useDefaultWebProxy="true" />
				</binding>
			</customBinding>

			<basicHttpBinding>
				<!-- See http://stackoverflow.com/questions/1520283/wcf-service-how-to-increase-the-timeout -->
				<binding name="BasicHttpBinding_IIdentityManager" openTimeout="00:03:00" sendTimeout="00:03:01" receiveTimeout="00:03:02" maxReceivedMessageSize="65536" maxBufferSize="65536" />
			</basicHttpBinding>
		</bindings>
